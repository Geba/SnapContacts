<Alloy>
	<Tab id="homeTab" titleid="home" class="tab">
		<Window id="homeWindow" titleid="home" layout="vertical">
			<SearchBar id="contatosSearch" class="searchBars" showCancel="false"></SearchBar>
			<Button id="Btadd" onClick="openAdd1"></Button>
			<TableView id="tableviewContatos" dataCollection="contato" height="Ti.UI.SIZE">
				<TableViewRow id="row" dataId="" title="{nome}" model="{alloy_id}" editable="true" layout="vertical">
					<View id="view" height="230">
						<Label id="rowName" text="{nome}"/>
					</View>
					<ImageView image="{foto1}"></ImageView>
				</TableViewRow>
			</TableView>
			<View layout="horizontal">
				<Label id="Lprocurar"></Label>
				<TextField id="TFprocurar"></TextField>
			</View>
		</Window>
	</Tab>
</Alloy>